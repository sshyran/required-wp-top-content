{
  "name": "wearerequired/required-wp-top-content",
  "description": "WordPress Top Content Plugin - Using Google Analytics Data",
  "version": "1.0.0",
  "type": "wordpress-plugin",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Stefan Pasch",
      "email": "stefan@required.ch",
      "homepage": "http://required.ch",
      "role": "Developer"
    }
  ],
  "config": {
    "vendor-dir": "./includes/vendor"
  },
  "require": {
    "google/apiclient": "1.1.7"
  },
  "repositories" : [
    {
      "type": "package",
      "package": {
        "name": "google/apiclient",
        "version": "1.1.7",
        "source": {
          "url": "git@github.com:google/google-api-php-client.git",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ]
}
